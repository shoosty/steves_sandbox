<form action="/users/<%= @user.id %>" method="POST">
  ID:     <input type="hidden" name="id" value= "<%= @user.id %>"  ><br>
  First:  <input type='text' name="user[first_name]" value= "<%= @user.first_name %>" ><br>
  Last:   <input type='text' name="user[last_name]" value= "<%= @user.last_name %>" ><br>
  email:  <input type='text' name="user[email]" value= "<%= @user.email %>" ><br>
  age:    <input type='text' name="user[age]"   value= "<%= @user.age %>"  ><br>
  <br>
  <input type="hidden" name="_method" value="patch" >
  <input type="submit" value="Submit" >
</form>

<br>
